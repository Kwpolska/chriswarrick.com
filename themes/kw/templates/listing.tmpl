## -*- coding: utf-8 -*-
<%inherit file="base.tmpl"/>

<%block name="content">
${ui.bar(crumbs)}
<ul class="unstyled">
% for name in folders:
    <li><a href="${name}"><i class="fa-folder-open"></i> ${name}</a>
% endfor
% for name in files:
    <li><a href="${name}.html"><i class="fa-file"></i> ${name}</a>
% endfor
</ul>
% if code:
    ${code}
% endif
</%block>

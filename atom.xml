---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Kw's Blog</title>
 <link href="http://kwpolska.tk/atom.xml" rel="self"/>
 <link href="http://kwpolska.tk/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://kwpolska.tk/</id>
 <author>
   <name>Kwpolska</name>
   <email>kwpolska@gmail.com</email>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://kwpolska.tk{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://kwpolska.tk{{ post.id }}</id>
   <content type="html">{{ post.content | strip_html | xml_escape | truncatewords: 25 }} Read more at the blog: http://kwpolska.tk{{ post.url }}</content> </entry>
 {% endfor %}
</feed>

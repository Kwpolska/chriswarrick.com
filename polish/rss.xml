---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
<channel>
 <title>Blog Kw</title>
 <link>http://kwpolska.co.cc/</link>
 <lastBuildDate>{{ site.time | date_to_xmlschema }}</lastBuildDate>
 <pubDate>{{ site.time | date_to_xmlschema }}</pubDate>
 {% for post in site.posts %}
 {% if post.category == "polish" %}
 <item>
   <title>{{ post.title }}</title>
   <link href="http://kwpolska.co.cc{{ post.url }}"/>
   <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
   <guid>http://kwpolska.co.cc{{ post.id }}</guid>
   <description>{{ post.content | strip_html | xml_escape | truncatewords: 25 }} <a href="http://kwpolska.co.cc{{ post.url }}">Read more</a></description>
 </item>
 {% endif %}
 {% endfor %}
</channel>
</rss>

---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
<channel>
 <title>Kw's Blog</title>
 <description>The Weblog of a Pole.</description>
 <link>http://kwpolska.tk/</link>
 <lastBuildDate>{{ site.time | date site.time, "%a, %d %b %Y %H:%M:%S +0100"}}</lastBuildDate>
 <pubDate>Sun, 13 Dec 2010 00:00:00 +0100</pubDate>
 {% for post in site.posts %}
 {% unless post.category == "polish" %}
 <item>
   <title>{{ post.title }}</title>
   <link>http://kwpolska.tk{{ post.url }}</link>
   <pubDate>{{ post.date | date post.date, "%a, %d %b %Y %H:%M:%S +0100"}}</pubDate>
   <guid>http://kwpolska.tk{{ post.id }}</guid>
   <description>{{ post.content | strip_html | xml_escape | truncatewords: 25 }} Read more at the blog: http://kwpolska.tk{{ post.url }}</description>
 </item>
 {% endunless %}
 {% endfor %}
</channel>
</rss>

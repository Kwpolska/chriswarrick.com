RewriteEngine on
RewriteBase /
RewriteRule ^projects/?$ http://kwpolska.github.io/projects/
RewriteRule ^projects/(.*) http://kwpolska.github.io/projects/$1
RewriteRule ^atom.xml http://kwpolska.tk/rss.xml
RewriteRule ^blog/tags/(.*) http://kwpolska.tk/blog/organizational_structures/$1

AddType text/css .css
AddOutputFilterByType DEFLATE text/html text/plain text/xml text/css

ErrorDocument 403 /err/403/index.html
ErrorDocument 404 /err/404/index.html
ErrorDocument 500 /err/500/index.html
ErrorDocument 503 /err/503/index.html
